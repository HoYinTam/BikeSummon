<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hosin.bikesummon.TestActivity">

    <android.support.v7.widget.Toolbar
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mToolbar"
        android:background="@color/colorPrimary"
        app:navigationIcon="@drawable/ic_action_nav"
        app:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        style="@style/AppTheme"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/testMapview"
        android:layout_alignStart="@+id/testMapview"/>



    <com.baidu.mapapi.map.TextureMapView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/testMapview"
        android:clickable="true"
        android:layout_below="@+id/toolbar"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
